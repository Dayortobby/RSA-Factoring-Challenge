import sys

def factorize(n):
    """
    Given an integer n, returns a tuple of two factors whose product is n.
    """
    for i in range(2, int(n**0.5)+1):
        if n % i == 0:
            return (i, n // i)
    return (n, 1)

if __name__ == '__main__':
    filename = sys.argv[1]
    with open(filename) as f:
        for line in f:
            n = int(line.strip())
            p, q = factorize(n)
            print(f"{n}={p}*{q}")
